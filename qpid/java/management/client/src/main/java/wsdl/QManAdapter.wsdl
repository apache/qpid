			<wsdl:output 
				wsa:Action="http://schemas.xmlsoap.org/ws/2004/09/mex/GetMetadataResponse" 
			    name="GetMetadataResponseMsg" 
			    message="qman:GetMetadataResponseMessage"/>
		</wsdl:operation>
